#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-125-g95444b4)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xe3eb30 .scope module, "mod3_tb" "mod3_tb" 2 2;
 .timescale -9 -12;
v0xe52e60_0 .var "clk", 0 0;
v0xe52f20_0 .var "input_num", 0 0;
v0xe52ff0_0 .net "res", 0 0, v0xe52c60_0;  1 drivers
v0xe530f0_0 .var "rstb", 0 0;
E_0xe3f120 .event posedge, v0xe05000_0;
S_0xe3ecb0 .scope module, "mod3_instance" "mod3" 2 29, 3 1 0, S_0xe3eb30;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rstb"
    .port_info 2 /INPUT 1 "input_num"
    .port_info 3 /OUTPUT 1 "res"
P_0xe04cf0 .param/l "S0" 1 3 7, C4<00>;
P_0xe04d30 .param/l "S1" 1 3 8, C4<01>;
P_0xe04d70 .param/l "S2" 1 3 9, C4<10>;
P_0xe04db0 .param/l "S3" 1 3 10, C4<11>;
v0xe05000_0 .net "clk", 0 0, v0xe52e60_0;  1 drivers
v0xe528c0_0 .net "input_num", 0 0, v0xe52f20_0;  1 drivers
v0xe52980_0 .var "next_state", 1 0;
v0xe52a70_0 .var "now_state", 1 0;
v0xe52b50_0 .var "num", 0 0;
v0xe52c60_0 .var "res", 0 0;
v0xe52d20_0 .net "rstb", 0 0, v0xe530f0_0;  1 drivers
E_0xe3f3e0/0 .event negedge, v0xe52d20_0;
E_0xe3f3e0/1 .event posedge, v0xe05000_0;
E_0xe3f3e0 .event/or E_0xe3f3e0/0, E_0xe3f3e0/1;
E_0xe3f2d0 .event edge, v0xe52a70_0, v0xe52b50_0;
    .scope S_0xe3ecb0;
T_0 ;
    %wait E_0xe3f2d0;
    %load/vec4 v0xe52a70_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0xe52b50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
    %jmp T_0.7;
T_0.6 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
T_0.7 ;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0xe52b50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.8, 8;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
    %jmp T_0.9;
T_0.8 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
T_0.9 ;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0xe52b50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.10, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
    %jmp T_0.11;
T_0.10 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
T_0.11 ;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0xe52b50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.12, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
    %jmp T_0.13;
T_0.12 ;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0xe52980_0, 0, 2;
T_0.13 ;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xe3ecb0;
T_1 ;
    %wait E_0xe3f3e0;
    %load/vec4 v0xe52d20_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0xe52a70_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0xe52980_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xe52b50_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0xe528c0_0;
    %assign/vec4 v0xe52b50_0, 0;
    %load/vec4 v0xe52980_0;
    %assign/vec4 v0xe52a70_0, 0;
    %load/vec4 v0xe52a70_0;
    %cmpi/e 3, 0, 2;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xe52c60_0, 0, 1;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xe52c60_0, 0, 1;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0xe3eb30;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xe52e60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xe530f0_0, 0, 1;
    %delay 50000, 0;
    %load/vec4 v0xe530f0_0;
    %inv;
    %store/vec4 v0xe530f0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0xe3eb30;
T_3 ;
    %delay 50000, 0;
    %load/vec4 v0xe52e60_0;
    %inv;
    %store/vec4 v0xe52e60_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0xe3eb30;
T_4 ;
    %wait E_0xe3f120;
    %vpi_func 2 25 "$random" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %store/vec4 v0xe52f20_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0xe3eb30;
T_5 ;
    %vpi_call 2 36 "$dumpfile", "mod3_tb.vcd" {0 0 0};
    %vpi_call 2 37 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xe3eb30 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0xe3eb30;
T_6 ;
    %delay 10000000, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_mod3.v";
    "mod3.v";

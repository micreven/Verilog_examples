:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x208fd90 .scope module, "seq_check_tb" "seq_check_tb" 2 2;
 .timescale -9 -12;
v0x20dfb60_0 .var "clk", 0 0;
v0x20dfc20_0 .var "in_a", 0 0;
v0x20dfcf0_0 .net "out_b", 0 0, v0x20df830_0;  1 drivers
v0x20dfdf0_0 .var "rstb", 0 0;
E_0x20904c0 .event edge, v0x20b6790_0;
S_0x208ff10 .scope module, "seq_check_instance" "seq_check" 2 27, 3 1 0, S_0x208fd90;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rstb"
    .port_info 2 /INPUT 1 "in_a"
    .port_info 3 /OUTPUT 1 "out_b"
P_0x20c70e0 .param/l "S0" 1 3 12, C4<000>;
P_0x20c7120 .param/l "S1" 1 3 13, C4<001>;
P_0x20c7160 .param/l "S2" 1 3 14, C4<010>;
P_0x20c71a0 .param/l "S3" 1 3 15, C4<011>;
P_0x20c71e0 .param/l "S4" 1 3 16, C4<100>;
v0x20b6790_0 .net "clk", 0 0, v0x20dfb60_0;  1 drivers
v0x20df680_0 .net "in_a", 0 0, v0x20dfc20_0;  1 drivers
v0x20df740_0 .var "now_state", 2 0;
v0x20df830_0 .var "out_b", 0 0;
v0x20df8f0_0 .net "rstb", 0 0, v0x20dfdf0_0;  1 drivers
v0x20dfa00_0 .var "state_nxt", 2 0;
E_0x2090240/0 .event negedge, v0x20df8f0_0;
E_0x2090240/1 .event posedge, v0x20b6790_0;
E_0x2090240 .event/or E_0x2090240/0, E_0x2090240/1;
E_0x20c7930 .event edge, v0x20df740_0, v0x20df680_0;
    .scope S_0x208ff10;
T_0 ;
    %wait E_0x20c7930;
    %load/vec4 v0x20df740_0;
    %store/vec4 v0x20dfa00_0, 0, 3;
    %load/vec4 v0x20df740_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %jmp T_0.6;
T_0.0 ;
    %load/vec4 v0x20df680_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.7, 8;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
    %jmp T_0.8;
T_0.7 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
T_0.8 ;
    %jmp T_0.6;
T_0.1 ;
    %load/vec4 v0x20df680_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.9, 8;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
    %jmp T_0.10;
T_0.9 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
T_0.10 ;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x20df680_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.11, 8;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
    %jmp T_0.12;
T_0.11 ;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
T_0.12 ;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x20df680_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.13, 8;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
    %jmp T_0.14;
T_0.13 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
T_0.14 ;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x20df680_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.15, 8;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
    %jmp T_0.16;
T_0.15 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x20dfa00_0, 0, 3;
T_0.16 ;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x208ff10;
T_1 ;
    %wait E_0x2090240;
    %load/vec4 v0x20df8f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x20df740_0, 0, 3;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x20dfa00_0;
    %assign/vec4 v0x20df740_0, 0;
    %load/vec4 v0x20df740_0;
    %pushi/vec4 4, 0, 3;
    %cmp/e;
    %flag_get/vec4 6;
    %assign/vec4 v0x20df830_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x208fd90;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x20dfb60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x20dfdf0_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x20dfdf0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x208fd90;
T_3 ;
    %delay 50000, 0;
    %load/vec4 v0x20dfb60_0;
    %inv;
    %store/vec4 v0x20dfb60_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x208fd90;
T_4 ;
    %wait E_0x20904c0;
    %vpi_func 2 24 "$random" 32 {0 0 0};
    %pushi/vec4 255, 0, 32;
    %mod/s;
    %pad/s 1;
    %store/vec4 v0x20dfc20_0, 0, 1;
    %jmp T_4;
    .thread T_4, $push;
    .scope S_0x208fd90;
T_5 ;
    %vpi_call 2 35 "$dumpfile", "tb_seq_check.vcd" {0 0 0};
    %vpi_call 2 36 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x208fd90 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x208fd90;
T_6 ;
    %delay 100000000, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
